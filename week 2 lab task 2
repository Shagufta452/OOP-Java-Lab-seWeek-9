TASK 3:
package taskk3;
public class Taskk3 {
 public static void main(String[] args) {
 Computer cs=new Computer("Apple","i5",50000,"IOS");
 cs.display();
 }

}
package taskk3;
class Computer {
 String name;
 String configuration;
 int cost;
 String operatingSystem;
 public Computer(String name, String configuration, int cost, String operatingSystem) {
 this.name = name;
 this.configuration = configuration;
 this.cost = cost;
 this. operatingSystem = operatingSystem;
 }
 public void display(){
 System.out.println("The company name "+name);
 System.out.println("The configuration is "+configuration);
 System.out.println("Its Cost is "+cost);
 System.out.println("Its operating System is "+operatingSystem);


 }

}


TASK 4:
package task4;
public class Task4 {
 int a;
 int b;
 int c;
 public Task4(int a, int b, int c) {
 this.a = a;
 this.b = b;
 this.c = c;
 }
 public void maximum() {
 if (a > b && a > c) {
 System.out.println("a is the maximum");
 } else if (b > a && b > c) {
 System.out.println("b is the maximum");
 } else if (c > a && c > b) {
 System.out.println("c is the maximum");
 } else {
 System.out.println("sorry.");
 }
 }
 public static void main(String[] args) {
 Task4 task = new Task4(2,3,1);
 task.maximum();
 }
}
