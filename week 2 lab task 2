TASK 1
package task.pkg1;
public class Task1 {

 public static void main(String[] args) {
 Student s1=new Student("shagufta",19,"islamabad");
 s1.display();
 Student s2=new Student("ayesha",20,"islamabad");
 s2.display();
 }

}
package task.pkg1;
public class Student {
 String name;
 int age;
 String city;
 public Student(String name, int age, String city) {
 this.name = name;
 this.age = age;
 this.city = city;
 }

 public void display(){
 System.out.println("Name: " + name);
 System.out.println("age: " + age);
 System.out.println("city isl: " +city);
 }

}
